%p#notice= notice
%h1 Listing Configs
%table
  %thead
    %tr
      %th Key
      %th Value
      %th{:colspan => "3"}
  %tbody
    - @configs.each do |config|
      %tr
        %td= config.key
        %td= config.value
        %td= link_to 'Show', config
        %td= link_to 'Edit', edit_config_path(config)
        %td= link_to 'Destroy', config, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Config', new_config_path
